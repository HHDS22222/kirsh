<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<properties resource="config/db.properties" />
	<typeAliases>
		<typeAlias type="com.kirsh.www.biz.user.UserVO" alias="user"></typeAlias>
		<typeAlias type="com.kirsh.www.biz.product.ProductVO" alias="product"></typeAlias>
		<typeAlias type="com.kirsh.www.biz.category.CategoryVO" alias="category"/>
		<typeAlias type="com.kirsh.www.biz.cart.CartVO" alias="cart"/>
		<typeAlias type="com.kirsh.www.biz.order.OrderVO" alias="order"/>
		<typeAlias type="com.kirsh.www.biz.orderDetail.OrderDetailVO" alias="orderDetail"/>
	</typeAliases>
	<mappers>
		<mapper resource="mappings/user-mapping.xml" />
		<mapper resource="mappings/product-mapping.xml" />
		<mapper resource="mappings/category-mapping.xml" />
		<mapper resource="mappings/cart-mapping.xml"/>
		<mapper resource="mappings/order-mapping.xml"/>
		<mapper resource="mappings/orderDetail-mapping.xml"/>
	</mappers>
</configuration>
